{
  "name": "@scintilla-network/sdk",
  "version": "1.2.0",
  "description": "A TypeScript SDK for Node.js and Web Browsers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "browser": "./src/index.js",
      "import": "./src/index.js",
      "require": "./src/index.js"
    }
  },
  "scripts": {
    "test": "litest",
    "test:watch": "litest watch",
    "lint": "eslint src --ext .ts",
    "test:asset": "litest src/primitives/Asset/Asset.spec.js",
    "test:authorization": "litest src/primitives/Authorization/Authorization.spec.js",
    "test:clusterblock": "litest src/primitives/ClusterBlock/ClusterBlock.spec.js",
    "test:drivedata": "litest src/primitives/DriveData/DriveData.spec.js",
    "test:governanceproposal": "litest src/primitives/GovernanceProposal/GovernanceProposal.spec.js",
    "test:governancevote": "litest src/primitives/GovernanceVote/GovernanceVote.spec.js",
    "test:hashproof": "litest src/primitives/HashProof/HashProof.spec.js",
    "test:transaction": "litest src/primitives/Transaction/Transaction.spec.js",
    "test:transition": "litest src/primitives/Transition/Transition.spec.js",
    "test:transfer": "litest src/primitives/Transfer/Transfer.spec.js",
    "test:quorumdecision": "litest src/primitives/QuorumDecision/QuorumDecision.spec.js",
    "test:quorumdecisionvote": "litest src/primitives/QuorumDecisionVote/QuorumDecisionVote.spec.js",
    "test:voucher": "litest src/primitives/Voucher/Voucher.spec.js",
    "test:identity": "litest src/primitives/Identity/Identity.spec.js",
    "test:netmessage": "litest src/primitives/messages/NetMessage/NetMessage.spec.js",
    "test:relayblock": "litest src/primitives/RelayBlock/RelayBlock.spec.js",
    "test:stateactiondata": "litest src/primitives/StateActionData/StateActionData.spec.js",
    "test:instruction": "litest src/primitives/Instruction/Instruction.spec.js",
    "test:serialize": "litest src/utils/serializeJSONToUint8Array.spec.js && litest src/utils/deserializeJSONFromUint8Array.spec.js",
    "test:fromString": "litest src/utils/serialize/fromString.spec.js",
    "test:fromVarInt": "litest src/utils/serialize/fromVarInt.spec.js",
    "test:fromVarBigInt": "litest src/utils/serialize/fromVarBigInt.spec.js",
    "test:fromBoolean": "litest src/utils/serialize/fromBoolean.spec.js",
    "test:fromArray": "litest src/utils/serialize/fromArray.spec.js",
    "test:fromObject": "litest src/utils/serialize/fromObject.spec.js",
    "test:toString": "litest src/utils/deserialize/toString.spec.js",
    "test:toVarInt": "litest src/utils/deserialize/toVarInt.spec.js",
    "test:toVarBigInt": "litest src/utils/deserialize/toVarBigInt.spec.js",
    "test:toBoolean": "litest src/utils/deserialize/toBoolean.spec.js",
    "test:toArray": "litest src/utils/deserialize/toArray.spec.js",
    "test:toObject": "litest src/utils/deserialize/toObject.spec.js",
    "test:roundtrip": "litest src/utils/roundtrip.spec.js",
    "test:all-serialize": "npm run test:fromString && npm run test:fromVarInt && npm run test:fromVarBigInt && npm run test:fromBoolean && npm run test:fromArray && npm run test:fromObject",
    "test:all-deserialize": "npm run test:toString && npm run test:toVarInt && npm run test:toVarBigInt && npm run test:toBoolean && npm run test:toArray && npm run test:toObject"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Scintilla-Network/sdk/issues"
  },
  "homepage": "https://github.com/Scintilla-Network/sdk#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Scintilla-Network"
  },
  "keywords": [
    "scintilla",
    "sdk",
    "javascript",
    "blockchain",
    "crypto",
    "assets",
    "governance",
    "transactions",
    "messages",
    "chainblocks",
    "clusterblocks",
    "drivedata",
    "governanceproposals",
    "governancevotes",
    "hashproofs",
    "peers",
    "vouchers",
    "transitions",
    "transfers",
    "transactions",
    "identities",
    "relayblocks"
  ],
  "author": "Scintilla Labs",
  "devDependencies": {
    "@scintilla-network/litest": "1.3.1",
    "@scintilla-network/wallet": "^1.0.0-beta.3"
  },
  "dependencies": {
    "@scintilla-network/hashes": "1.3.1",
    "@scintilla-network/keys": "1.0.0-beta.26",
    "@truestamp/tree": "2.1.0",
    "hermodlog": "1.6.0"
  }
}
